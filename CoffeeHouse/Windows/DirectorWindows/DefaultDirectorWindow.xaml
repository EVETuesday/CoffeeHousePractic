<Window x:Class="CoffeeHouse.Windows.Director.DefaultDirectorWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:CoffeeHouse.Windows.Director"
        mc:Ignorable="d"
        ResizeMode="NoResize"
        WindowStyle="ToolWindow"
        WindowStartupLocation="CenterScreen"
        Title="DefaultDirectorWindow" Height="700" Width="400">
    <Grid>
        <Grid.Background>
            <ImageBrush ImageSource="/Resourses/Images/JpgFormat/1614414667_11-p-chashka-kofe-na-temnom-fone-17.jpg"/>
        </Grid.Background>

        <Frame x:Name="DirectorFrame"
               
                NavigationUIVisibility="Hidden"
                Margin="15"/>
        <Button x:Name="btnBack"
                Height="20"
                Width="20"
                Content=">"
                FontSize="10"
                Padding="0 0 0 1"
                HorizontalAlignment="Right"
                VerticalAlignment="Top"
                Margin="2.5"
                Background="Black"
                BorderBrush="#FF790303"
                Foreground="White"
                Click="btnBack_Click"/>
    </Grid>
</Window>
